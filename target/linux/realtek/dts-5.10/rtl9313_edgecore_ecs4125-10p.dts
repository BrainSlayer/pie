/dts-v1/;

#include "rtl931x.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	compatible = "edgecore,ecs4125-10p", "realtek,rtl838x-soc";
	model = "Edgecore ECS4125-12P Switch";

	aliases {
		led-boot = &led_sys;
		led-failsafe = &led_sys;
		led-running = &led_sys;
		led-upgrade = &led_sys;
	};	


	memory@0 {
		device_type = "memory";
		reg = <0x0 0x10000000>;
	};

	memory@90000000 {
		device_type = "memory";
		reg = <0x90000000 0x30000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	leds {
		compatible = "gpio-leds";

		led_sys: sys {
			label = "ecs4125_15:green:sys";
			gpios = <&gpio0 15 GPIO_ACTIVE_HIGH>;
		};
	};
};

&gpio0 {
	indirect-access-bus-id = <0>;
};

&spi0 {
	status = "okay";
	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0xe0000>;
				read-only;
			};
			partition@100000 {
				label = "bdinfo";
				reg = <0x100000 0x10000>;
				read-only;
			};
			partition@110000 {
				label = "u-boot-env";
				reg = <0x110000 0x10000>;
				read-only;
			};
			partition@120000 {
				label = "jffs";
				reg = <0x120000 0x100000>;
			};
			partition@220000 {
				label = "jffs2";
				reg = <0x220000 0x100000>;
			};
			partition@320000 {
				label = "runtime";
				reg = <0x320000 0x1e80000>;
				compatible = "denx,uimage";
			};
			partition@2200000 {
				label = "runtime2";
				reg = <0x2200000 0x1e00000>;
			};
		};
	};
};

&ethernet0 {
	mdio: mdio-bus {
		compatible = "realtek,rtl838x-mdio";
		regmap = <&ethernet0>;
		#address-cells = <1>;
		#size-cells = <0>;
		
		/* External phy ARQ813 */
		phy0: ethernet-phy@0 {
			reg = <0>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 0>;
		};
		phy8: ethernet-phy@8 {
			reg = <8>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 1>;
		};
		phy16: ethernet-phy@16 {
			reg = <16>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 2>;
		};
		phy24: ethernet-phy@24 {
			reg = <24>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 3>;
		};
		phy32: ethernet-phy@32 {
			reg = <32>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 4>;
		};
		phy40: ethernet-phy@40 {
			reg = <40>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 5>;
		};
		phy48: ethernet-phy@48 {
			reg = <48>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 6>;
		};
		phy50: ethernet-phy@50 {
			reg = <50>;
			compatible = "ethernet-phy-ieee802.3-c22";
			rtl9300,smi-address= <0 7>;
		};

		/* RTL9313 Internal Serdes */
		phy54: ethernet-phy@54 {
			compatible = "ethernet-phy-ieee802.3-c22";
			phy-is-integrated;
			reg = <54>;
		};
		phy55: ethernet-phy@55 {
			compatible = "ethernet-phy-ieee802.3-c22";
			phy-is-integrated;
			reg = <55>;
		};

	};
};

&switch0 {
	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			label = "lan1";
			phy-handle = <&phy0>;
			phy-mode = "qsgmii";
		};
		port@8 {
			reg = <8>;
			label = "lan2";
			phy-handle = <&phy8>;
			phy-mode = "qsgmii";
		};
		port@16 {
			reg = <16>;
			label = "lan3";
			phy-handle = <&phy16>;
			phy-mode = "qsgmii";
		};
		port@24 {
			reg = <24>;
			label = "lan4";
			phy-handle = <&phy24>;
			phy-mode = "qsgmii";
		};
		port@32 {
			reg = <32>;
			label = "lan5";
			phy-handle = <&phy32>;
			phy-mode = "qsgmii";
		};
		port@40 {
			reg = <40>;
			label = "lan6";
			phy-handle = <&phy40>;
			phy-mode = "qsgmii";
		};
		port@48 {
			reg = <48>;
			label = "lan7";
			phy-handle = <&phy48>;
			phy-mode = "qsgmii";
		};
		port@50 {
			reg = <50>;
			label = "lan8";
			phy-handle = <&phy50>;
			phy-mode = "qsgmii";
		};

		/* RTL9313 Internal SerDes */
		port@54 {
			reg = <54>;
			label = "lan54";
			phy-mode = "internal";
			phy-handle = <&phy54>;
		};
		port@55 {
			reg = <55>;
			label = "lan55";
			phy-mode = "internal";
			phy-handle = <&phy55>;
		};

		/* CPU-Port */
		port@56 {
			ethernet = <&ethernet0>;
			reg = <56>;
			phy-mode = "qsgmii";
			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};
	};
};
